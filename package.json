{
  "name": "easy_cms",
  "version": "1.0.0",
  "description": "drag to create content manage system",
  "main": "index.js",
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "webpack serve --config webpack.config.dev.js --open",
    "dev:wrangler": "npx wrangler pages dev dist_client",
    "build": "webpack --config webpack.config.prod.js",
    "build:server": "rm -rf dist_server && tsc --project server/tsconfig.server.json",
    "start:dev": "cross-env NODE_ENV=dev nodemon --exec ts-node --project server/tsconfig.server.json server/index.ts",
    "start:prod": "cross-env NODE_ENV=prod nodemon --exec ts-node --project server/tsconfig.server.json server/index.ts",
    "deploy": "npm run start:prod"
  },
  "keywords": [
    "ts",
    "react"
  ],
  "author": "deeprainsuffix",
  "license": "ISC",
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.12",
    "jszip": "^3.10.1",
    "lucide-react": "^0.403.0",
    "monaco-editor": "^0.51.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-router-dom": "^6.26.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240821.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.39",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1",
    "wrangler": "^3.73.0"
  }
}